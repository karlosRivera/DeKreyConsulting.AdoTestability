      "frameworkAssemblies": {
        "System.Xml": "[4.0.0.0,)",
        "System.Xml.Linq": "[4.0.0.0,)"
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Data.SqlClient": "[4.3.0,)",
        "System.Xml.XDocument": "[4.3.0,)",
        "System.Xml.XPath.XmlDocument": "[4.3.0,)"
      }
    }
  },
  "dependencies": {
    "DeKreyConsulting.AdoTestability": "[1.1.0,)",
    "DeKreyConsulting.AdoTestability.Testing.Stubs": "[1.1.0,)"
  }
